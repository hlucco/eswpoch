{
  "name": "epoch",
  "displayName": "Eswpoch (Epoch Converter)",
  "version": "1.0.0",
  "publisher": "hlucco",
  "description": "Lightweight VS Code plugin for executing inline swaps between timestamps and viewing corresponding GMT time.",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:eswpoch.epochConvert"
  ],
  "main": "./out/eswpoch.js",
  "contributes": {
    "commands": [
      {
        "command": "eswpoch.epochConvert",
        "title": "Epoch Converter"
      }
    ],
    "keybindings": [
      {
        "command": "eswpoch.epochConvert",
        "key": "ctrl+alt+e"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext ts, .tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.34.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "typescript": "^4.7.4"
  },
  "author": "hlucco",
  "license": "MIT",
  "repository": "https://www.github.com/hlucco/eswpoch",
  "icon": "clock-icon.png"
}
